{
	"name": "snipkey",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"deploy": "bun run build && wrangler deploy",
		"lint": "biome lint .",
		"format": "biome format . --write",
		"check": "biome check . --write --unsafe && tsc --noEmit",
		"generate": "wrangler types --env-interface CloudflareEnv --include-runtime=false && wrangler types --include-env=false worker-runtime.d.ts",
		"db:migrate": "wrangler d1 migrations apply snipkey-db",
		"db:migration:create": "wrangler d1 migrations create snipkey-db",
		"db:migration:list": "wrangler d1 migrations list snipkey-db"
	},
	"dependencies": {
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-collapsible": "^1.1.12",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-slot": "^1.2.4",
		"@simplewebauthn/browser": "^13.2.2",
		"@simplewebauthn/server": "^13.2.2",
		"@tanstack/react-router": "^1.150.0",
		"@tanstack/react-router-devtools": "^1.150.0",
		"@tanstack/start": "^1.120.20",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"kysely": "^0.28.9",
		"kysely-d1": "^0.4.0",
		"lucide-react": "^0.562.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"use-local-storage-state": "^19.5.0",
		"vite": "^7.3.1",
		"vite-tsconfig-paths": "^6.0.4",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@cloudflare/vite-plugin": "^1.21.0",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-start": "^1.150.0",
		"@tanstack/router-plugin": "^1.150.0",
		"@tanstack/vite-config": "^0.4.3",
		"@types/bun": "latest",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"autoprefixer": "^10.4.23",
		"babel-plugin-react-compiler": "^1.0.0",
		"eslint-plugin-react-hooks": "^7.0.1",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"wrangler": "^4.59.2"
	}
}
